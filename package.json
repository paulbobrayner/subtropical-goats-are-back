  "bugs": {
    "url": "https://github.com/paulbobrayner/subtropical-goats-are-back/issues"
  },
  "homepage": "https://github.com/paulbobrayner/subtropical-goats-are-back#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.15.2",
    "pg": "^7.18.2",
    "socket.io": "^4.2.0"
  }
}
